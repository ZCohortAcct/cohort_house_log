<h1>Edit your <%= @room_obj.name %> room here.</h1>

 <form method='POST' action='/rooms/<%= @room_obj.id %>'>
  <input type='hidden' name="_method" value='patch'>
  <label>Name</label> <input type='text' name='name' value=<%= @room_obj.name %> ><br/>
  <input type='text' name='theme' placeholder="Theme" value=<%= @room_obj.theme %> ><br/>
  <p> Click in circle if room is being utilized. 
    <input type="checkbox" name='use' value=<%= @room_obj.use %>
      <%= 'checked' if @room_obj.use %>
    >
  </p>
  <input type="submit" />
</form>
